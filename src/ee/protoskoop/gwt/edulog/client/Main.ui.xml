
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	/* Add CSS here. See the GWT docs on UI Binder for more details */
	</ui:style>
	<g:HTMLPanel>
		<div class="container">
			<h1 class="page-header">EduLog</h1>
			<hr></hr>
			<p
				style="color:#858585; text-align: center; font-size: 1.2em; font-style: italic;">
				<mark>Select</mark>
				feedback session
			</p>
			<div class="container" style="padding:0">
				<form>
					<div class="form-group">
						<label>Select session</label>
						<g:ListBox ui:field="listBoxSelectSession"
							styleName="form-control"></g:ListBox>
					</div>
				</form>
				<br></br>
				<g:Button ui:field="buttonSelectSession"
					styleName="form-control">Select</g:Button>
				<br></br>
				<form>
					<div class="form-group">
						<label>Select activity</label>
						<g:ListBox ui:field="listBoxSelectActivity"
							styleName="form-control"></g:ListBox>
					</div>
				</form>
				<br></br>
				<g:Button ui:field="buttonSelectActivity"
					styleName="form-control">Select</g:Button>
				<br></br>
			</div>
			<hr></hr>
			<p
				style="color:#858585; text-align: center; font-size: 1.2em; font-style: italic;">
				<mark>Request</mark>
				feedback
			</p>
			<g:Button ui:field="buttonStartFeedback"
				styleName="form-control">Start now</g:Button>
			<br></br>
			<g:Button ui:field="buttonBack" styleName="form-control">Back</g:Button>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 

